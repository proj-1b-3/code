<?xml version="1.0" standalone="yes"?>
<xs:schema id="DataBase" xmlns="" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xs:element name="DataBase" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element name="Users">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="UserName" type="xs:string" />
              <xs:element name="Password" type="xs:string" minOccurs="0" />
              <xs:element name="Role" msdata:DataType="App.Role, App, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null" type="xs:anyType" minOccurs="0" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="Rooms">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="RoomName" type="xs:string" />
              <xs:element name="Theme" type="xs:string" minOccurs="0" />
              <xs:element name="Discription" type="xs:string" minOccurs="0" />
              <xs:element name="Capacity" type="xs:int" minOccurs="0" />
              <xs:element name="Price" type="xs:float" minOccurs="0" />
              <xs:element name="Available" type="xs:boolean" minOccurs="0" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="Tickets">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="TicketId" msdata:DataType="System.Guid, System.Private.CoreLib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=7cec85d7bea7798e" type="xs:string" />
              <xs:element name="UserName" type="xs:string" minOccurs="0" />
              <xs:element name="RoomName" type="xs:string" minOccurs="0" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:choice>
    </xs:complexType>
    <xs:unique name="Constraint1" msdata:PrimaryKey="true">
      <xs:selector xpath=".//Users" />
      <xs:field xpath="UserName" />
    </xs:unique>
    <xs:unique name="Rooms_Constraint1" msdata:ConstraintName="Constraint1" msdata:PrimaryKey="true">
      <xs:selector xpath=".//Rooms" />
      <xs:field xpath="RoomName" />
    </xs:unique>
    <xs:unique name="Tickets_Constraint1" msdata:ConstraintName="Constraint1" msdata:PrimaryKey="true">
      <xs:selector xpath=".//Tickets" />
      <xs:field xpath="TicketId" />
    </xs:unique>
    <xs:unique name="Constraint2">
      <xs:selector xpath=".//Tickets" />
      <xs:field xpath="UserName" />
    </xs:unique>
    <xs:unique name="Constraint3">
      <xs:selector xpath=".//Tickets" />
      <xs:field xpath="RoomName" />
    </xs:unique>
    <xs:keyref name="TicketRoom" refer="Rooms_Constraint1">
      <xs:selector xpath=".//Tickets" />
      <xs:field xpath="RoomName" />
    </xs:keyref>
    <xs:keyref name="TicketOwner" refer="Constraint1">
      <xs:selector xpath=".//Tickets" />
      <xs:field xpath="UserName" />
    </xs:keyref>
  </xs:element>
</xs:schema>